<?xml version="1.0" encoding="UTF-8"?>
<suppressions
    xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.1.xsd">
    <suppress>
        <notes><![CDATA[
            file name: tomcat-annotations-api-8.5.23.jar
        ]]></notes>
        <gav regex="true">^org\.apache\.tomcat:tomcat-annotations-api:.*$</gav>
        <cpe>cpe:/a:apache:tomcat</cpe>
    </suppress>
    <suppress>
        <notes>last present in 24.1; fixed in 24.1.1 and 25.0</notes>
        <cpe>cpe:2.3:a:google:guava:*:*:*:*:*:*:*:*</cpe>
        <cve>CVE-2018-10237</cve>
    </suppress>
    <suppress>
        <notes><![CDATA[ relates to CETS ]]></notes>
        <gav regex="true">.*</gav>
        <cve>CVE-2018-8088</cve>
    </suppress>
    <suppress>
